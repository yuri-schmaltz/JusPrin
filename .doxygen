# Doxyfile for JusPrin

PROJECT_NAME           = "JusPrin"
PROJECT_BRIEF          = "Advanced 3D Printing Slicer"
PROJECT_VERSION        = 2.0.0

OUTPUT_DIRECTORY       = docs/api
CREATE_SUBDIRS         = YES

EXTRACT_ALL            = NO
EXTRACT_PRIVATE        = NO
EXTRACT_STATIC         = YES
EXTRACT_LOCAL_CLASSES  = NO

INPUT                  = src/libslic3r \
                         src/slic3r/GUI \
                         src/slic3r/Utils \
                         README.md

FILE_PATTERNS          = *.h \
                         *.hpp \
                         *.cpp \
                         *.md

RECURSIVE              = YES

EXCLUDE                = src/libslic3r/miniz \
                         deps \
                         deps_src \
                         build

EXCLUDE_PATTERNS       = */test/* \
                         */tests/* \
                         */.git/*

SOURCE_BROWSER         = YES
INLINE_SOURCES         = NO

GENERATE_HTML          = YES
HTML_OUTPUT            = html
HTML_FILE_EXTENSION    = .html
HTML_COLORSTYLE_HUE    = 220
HTML_COLORSTYLE_SAT    = 100
HTML_COLORSTYLE_GAMMA  = 80
HTML_TIMESTAMP         = YES
HTML_DYNAMIC_SECTIONS  = YES

GENERATE_LATEX         = NO

GENERATE_RTF           = NO
GENERATE_MAN           = NO
GENERATE_XML           = NO

ENABLE_PREPROCESSING   = YES
MACRO_EXPANSION        = YES
EXPAND_ONLY_PREDEF     = NO

PREDEFINED             = "__cplusplus" \
                         "SLIC3R_GUI" \
                         "HAS_BOOL" \
                         "_WIN32"

CLASS_DIAGRAMS         = YES
HAVE_DOT               = YES
DOT_NUM_THREADS        = 4
UML_LOOK               = YES
TEMPLATE_RELATIONS     = YES
CALL_GRAPH             = YES
CALLER_GRAPH           = YES
GRAPHICAL_HIERARCHY    = YES
DIRECTORY_GRAPH        = YES
DOT_IMAGE_FORMAT       = svg
INTERACTIVE_SVG        = YES

WARNINGS               = YES
WARN_IF_UNDOCUMENTED   = NO
WARN_IF_DOC_ERROR      = YES
WARN_NO_PARAMDOC       = NO

USE_MDFILE_AS_MAINPAGE = README.md
